@using Q3.Models
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>List All Books: </h1>

<div>
    <table border="1">
        <thead>
            <th>ID</th>
            <th>Title</th>
            <th>Year</th>
            <th>Description</th>
            <th>Action</th>
        </thead>
        <tbody>
            @foreach(Book s in @Model){
            <tr>
                <td>@s.Id</td>
                <td>@s.Title</td>
                <td>@s.Year</td>
                <td>@s.Description</td>
                <td><a href="Delete/@s.Id">Delete</a></td>
            </tr>
            }
        </tbody>
    </table>
</div>